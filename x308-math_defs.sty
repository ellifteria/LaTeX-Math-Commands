\usepackage{amsthm}

% theorem environment
\newtheorem{theorem}{Theorem}[section]
\newtheorem{lemma}[theorem]{Lemma}
\renewcommand\qedsymbol{$\blacksquare$}

% commands for usual sets of numbers
\newcommand{\naturals}{\mathbb{N}}
\newcommand{\integers}{\mathbb{Z}}
\newcommand{\rationals}{\mathbb{Q}}
\newcommand{\reals}{\mathbb{R}}
\newcommand{\complexes}{\mathbb{C}}

% commands for elementary number theory
\newcommand{\divides}{\bigm|}
\newcommand{\ndivides}{\mathrel{\mkern.5mu \ooalign{\hidewidth$\big|$\hidewidth\cr$\nmid$\cr}}}

% commands for model theory
\newcommand{\proves}{\vdash}
\newcommand{\nproves}{\nvdash}
\renewcommand\models{\vDash}
\newcommand{\nmodels}{\nvDash}
\newcommand{\submodel}{\preceq}
\newcommand{\nsubmodel}{\npreceq}
\newcommand{\peanoarithmetic}[1]{\mathcal{PA}_{#1}}
